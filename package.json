{
  "name": "windowbuilder",
  "version": "2.0.242",
  "description": "Order dealer for windows and stained glass",
  "repository": {
    "type": "git",
    "url": "https://github.com/oknosoft/windowbuilder.git"
  },
  "engines": {
    "node": "^8.8",
    "npm": "^6.0"
  },
  "keywords": [
    "metadata",
    "order dealer",
    "cad",
    "crm",
    "window builder"
  ],
  "author": "Evgeniy Malyarov <info@oknosoft.ru> (http://www.oknosoft.ru)",
  "license": "SEE LICENSE IN LICENSE.en.md",
  "private": true,
  "bugs": {
    "url": "https://github.com/oknosoft/windowbuilder/issues"
  },
  "homepage": "http://light.oknosoft.ru/",
  "proxy": "http://localhost:3030/",
  "dependencies": {
    "@jscad/openjscad": "^1.6.1",
    "dxf-writer": "^1.2.0",
    "genetic-cutting": "github:oknosoft/genetic-cutting",
    "metadata-abstract-ui": "2.0.17-beta.10",
    "metadata-core": "2.0.17-beta.10",
    "metadata-dhtmlx": "2.0.17-beta.10",
    "metadata-external": "2.0.17-beta.10",
    "metadata-pouchdb": "2.0.17-beta.10",
    "metadata-react": "2.0.17-beta.10",
    "metadata-redux": "2.0.17-beta.10",
    "paper-jsdom-canvas": "^0.11.8"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "^2.4.1",
    "css-loader": "0.28.11",
    "dotenv": "^6.1.0",
    "eslint": "^5.7.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^2.0.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "gulp": "^3.9",
    "gulp-base64": "^0.1",
    "gulp-concat": "^2.6",
    "gulp-csso": "^3.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0",
    "gulp-strip-comments": "^2.5.2",
    "gulp-umd": "^2.0",
    "gulp-wrap": "^0.14",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "md5-file": "^4.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0",
    "raf": "3.4.0",
    "raw-loader": "^0.5.1",
    "react": "^16.5.2",
    "react-dev-utils": "^5.0.3",
    "react-dom": "^16.5.2",
    "rimraf": "^2.6",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.1.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.3",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && node scripts/appcache.js",
    "test": "node scripts/test.js --env=jsdom",
    "meta:prebuild": "node scripts/meta.prebuild.js",
    "meta:link": "node scripts/meta.link.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
